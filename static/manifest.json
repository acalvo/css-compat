{
  "manifest_version": 2,
  "name": "CSS Compat",
  "description": "Shows a report of CSS compatibility issues across browsers.",
  "version": "0.2.0",
  "author": "Alberto Calvo",
  "homepage_url": "https://github.com/acalvo/css-compat",
  "icons": {
    "512": "images/logo.png"
  },
  "background": {
    "scripts": [
      "browser-polyfill.js",
      "background.js"
    ]
  },
  "permissions": [
    "<all_urls>"
  ],
  "devtools_page": "index.html"
}
